/g 9.81 def

/tg {
	dup sin exch cos div
} bind def

/sq {
	dup mul
} bind def

%V0 theta t
/parab_Vz {
	/t exch def
	/theta exch def
	/V0 exch def
	
	theta sin
	V0 mul
	t g mul
	sub
} def

%V0 theta t
/parab_Vx {
	/t exch def
	/theta exch def
	/V0 exch def
	
	theta cos
	V0 mul
} def


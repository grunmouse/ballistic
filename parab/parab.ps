} def

%V0 theta t
/parab_z {
	/t exch def
	/theta exch def
	/V0 exch def
	
	theta sin
	V0 mul
	t mul
	g t t mul mul
	sub
} def

%V0 theta t
/parab_theta {
	/t exch def
	/theta exch def
	/V0 exch def
	
	theta tg
	g t mul
	V0 theta cos mul
	div
	sub
} def

%V0 theta 
/parab_tC {
